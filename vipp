#!/usr/bin/env node
'use strict';

var vipp = require('./src/main');
var fs = require('fs');

function main() {
	// Parse args
	var args = process.argv.slice(2);
	var filename = args[0];
	if (filename === undefined) {
		console.log('usage: vipp filename');
		return;
	}
	// Run the thing
	var code = fs.readFileSync(filename);
	var fn = vipp.compile(code);
	console.log(fn());
}

main();