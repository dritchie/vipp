
var none = {
	fwd: function(x) { return x; },
	rvs: function(x) { return x; }
}

var nonNegative = {
	fwd: Math.exp,
	rvs: Math.log
};

var unitInterval = {
	fwd: function sigmoid(x) { return 1 / (1 + Math.exp(-x)); },
	rvs: function logit(x) { return -Math.log(1/x - 1); }
};

module.exports = {
	none: none,
	nonNegative: nonNegative,
	unitInterval: unitInterval
}